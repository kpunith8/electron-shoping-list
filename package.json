{
  "name": "electron-shoping-list",
  "version": "1.0.0",
  "description": "Shopping list using electron and JS",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-shoping-list --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Shoping List\"",
    "package-linux": "electron-packager . electron-shoping-list --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "keywords": [
    "electron",
    "javascript"
  ],
  "author": "Punith K",
  "license": "MIT",
  "dependencies": {
    "electron": "^15.5.5"
  },
  "devDependencies": {
    "electron-packager": "^12.2.0"
  }
}
